{
  "manifest_version": 2,

  "name": "Tab search",
  "description": "Search your tabs and switch using the keyboard",
  "version": "1.0.1",

  "permissions": [
    "tabs",
    "history"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Y"
      }
    }
  }
}